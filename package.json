{
  "name": "vite-demo",
  "version": "0.0.0",
  "scripts": {
    "build": "routify -b && vite build",
    "dev": "run-p dev:*",
    "dev:routify": "routify",
    "dev:vite": "vite",
    "format": "pretty-quick --ignore-path .gitignore",
    "format:check": "pretty-quick --check --ignore-path .gitignore",
    "serve": "vite preview",
    "svelte:check": "svelte-check --fail-on-warnings",
    "lint:check": "eslint --ignore-path .gitignore \"src/**/*.{ts,js,svelte}\"",
    "lint:fix": "eslint --fix --ignore-path .gitignore \"src/**/*.{ts,js,svelte}\""
  },
  "devDependencies": {
    "@roxi/routify": "^2.15.1",
    "@sveltejs/eslint-config": "github:sveltejs/eslint-config",
    "@svitejs/vite-plugin-svelte": "^0.11.1",
    "@typescript-eslint/eslint-plugin": "^4.18.0",
    "@typescript-eslint/parser": "^4.18.0",
    "eslint": "^7.22.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.22.1",
    "husky": "^5.1.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "prettier-plugin-svelte": "^2.2.0",
    "pretty-quick": "^3.1.0",
    "sass": "^1.32.8",
    "svelte": "^3.35.0",
    "svelte-check": "^1.2.5",
    "svelte-preprocess": "^4.6.9",
    "typescript": "^4.2.3",
    "vite": "^2.1.0",
    "vite-plugin-windicss": "^0.9.2",
    "windicss": "^2.5.1"
  },
  "dependencies": {
    "eslint-import-resolver-custom-alias": "^1.2.0"
  }
}
