{
  "name": "svelte-kit-demo",
  "version": "0.0.1",
  "scripts": {
    "dev": "svelte-kit dev",
    "build": "svelte-kit build",
    "start": "svelte-kit start",
    "format": "pretty-quick --ignore-path .gitignore",
    "format:check": "pretty-quick --check --ignore-path .gitignore",
    "lint:check": "eslint --ignore-path .gitignore \"src/**/*.{ts,js,svelte}\"",
    "lint:fix": "eslint --fix --ignore-path .gitignore \"src/**/*.{ts,js,svelte}\"",
    "prepare": "husky install",
    "svelte:check": "svelte-check --fail-on-warnings"
  },
  "devDependencies": {
    "@sveltejs/adapter-node": "next",
    "@sveltejs/adapter-vercel": "next",
    "@sveltejs/eslint-config": "github:sveltejs/eslint-config",
    "@sveltejs/kit": "next",
    "@typescript-eslint/eslint-plugin": "^4.19.0",
    "@typescript-eslint/parser": "^4.19.0",
    "eslint": "^7.22.0",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-import": "^2.22.1",
    "husky": "^5.2.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "prettier-plugin-svelte": "^2.2.0",
    "pretty-quick": "^3.1.0",
    "sass": "^1.0.0",
    "svelte": "^3.29.0",
    "svelte-check": "^1.3.0",
    "svelte-preprocess": "^4.0.0",
    "svelte-windicss-preprocess": "^3.1.1",
    "tslib": "^2.0.0",
    "typescript": "^4.0.0",
    "vite": "^2.1.0",
    "windicss": "^2.5.4"
  },
  "engines": {
    "node": ">= 12.17.0"
  },
  "type": "module"
}
