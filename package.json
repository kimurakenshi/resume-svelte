{
	"name": "svelte-kit-demo",
	"version": "0.0.1",
	"scripts": {
		"build": "svelte-kit build",
		"dev": "svelte-kit dev",
		"format": "prettier --ignore-path .gitignore --write .",
		"format:check": "pretty-quick --ignore-path .gitignore --check",
		"format:staged": "pretty-quick --ignore-path .gitignore --staged",
		"lint:check": "eslint --ignore-path .gitignore .",
		"lint:fix": "eslint --ignore-path .gitignore --fix .",
		"lint:fix:staged": "eslint --ignore-path .gitignore --cache --fix .",
		"prepare": "husky install",
		"start": "svelte-kit start"
	},
	"devDependencies": {
		"@sveltejs/adapter-vercel": "next",
		"@sveltejs/kit": "next",
		"@tailwindcss/jit": "^0.1.1",
		"@tailwindcss/typography": "^0.4.0",
		"@typescript-eslint/eslint-plugin": "^4.19.0",
		"@typescript-eslint/parser": "^4.19.0",
		"autoprefixer": "^10.2.5",
		"cssnano": "^4.1.10",
		"eslint": "^7.22.0",
		"eslint-config-prettier": "^8.1.0",
		"eslint-plugin-svelte3": "^3.1.2",
		"husky": "^5.2.0",
		"mdsvex": "^0.8.9",
		"npm-run-all": "^4.1.5",
		"postcss": "^8.2.7",
		"postcss-load-config": "^3.0.1",
		"prettier": "^2.2.1",
		"prettier-plugin-svelte": "^2.2.0",
		"pretty-quick": "^3.1.0",
		"rehype-slug": "^4.0.1",
		"remark-abbr": "^1.4.1",
		"remark-github": "^10.0.1",
		"sass": "^1.0.0",
		"svelte": "^3.29.0",
		"svelte-preprocess": "^4.6.9",
		"tailwindcss": "^2.0.3",
		"tslib": "^2.0.0",
		"typescript": "^4.0.0",
		"vite": "^2.1.0"
	},
	"engines": {
		"node": ">= 12.17.0"
	},
	"type": "module"
}
