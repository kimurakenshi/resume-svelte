{
	"name": "svelte-kit-demo",
	"version": "0.0.1",
	"scripts": {
		"build": "svelte-kit build",
		"dev": "svelte-kit dev",
		"format": "prettier --ignore-path .gitignore --write .",
		"format:check": "pretty-quick --ignore-path .gitignore --check",
		"format:staged": "pretty-quick --ignore-path .gitignore --staged",
		"lint:check": "eslint --ignore-path .gitignore .",
		"lint:fix": "eslint --ignore-path .gitignore --fix .",
		"lint:fix:staged": "eslint --ignore-path .gitignore --cache --fix .",
		"prepare": "husky install",
		"start": "svelte-kit start"
	},
	"devDependencies": {
		"@sveltejs/adapter-vercel": "next",
		"@sveltejs/kit": "next",
		"@typescript-eslint/eslint-plugin": "^4.19.0",
		"@typescript-eslint/parser": "^4.19.0",
		"eslint": "^7.22.0",
		"eslint-config-prettier": "^8.1.0",
		"eslint-plugin-svelte3": "^3.1.2",
		"npm-run-all": "^4.1.5",
		"prettier": "^2.2.1",
		"prettier-plugin-svelte": "^2.2.0",
		"pretty-quick": "^3.1.0",
		"sass": "^1.0.0",
		"svelte": "^3.29.0",
		"svelte-preprocess": "^4.0.0",
		"svelte-windicss-preprocess": "^3.1.1",
		"tslib": "^2.0.0",
		"typescript": "^4.0.0",
		"vite": "^2.1.0",
		"windicss": "^2.5.4"
	},
	"engines": {
		"node": ">= 12.17.0"
	},
	"type": "module"
}
